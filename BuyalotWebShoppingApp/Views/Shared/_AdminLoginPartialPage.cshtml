@if (Session["adminName"] != null)
{
    <ul class="nav navbar-nav navbar-right">
        <li><a>Welcome Admin @Session["adminName"] </a></li>
        <li>@Html.ActionLink("Logout", "Logout", new { controller = "AdminAccount" }, new { @class = "logOut" })</li>
    </ul>

}
else
{

    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Login", "Login", new { controller = "AdminAccount" }, new { @class = "login" })</li>
        <li>@Html.ActionLink("Register", "Register", new { controller = "AdminAccount" }, new { @class = "register" })</li>
    </ul>
}
